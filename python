print("Hello world")
#string
mystring = 'hello'
print(mystring)
mystring = "world"
print(mystring)
#float
myfloat = 300.0
print(myfloat)
myfloat = float(7)
print(myfloat)
#list
mylist = [1,2,3]
mylist.append(1)
mylist.append(2)
mylist.append(3)
print(mylist[0])
print(mylist[1])
print(mylist[2])

for x in mylist:
    print(x)
